virtual void BaseItem::setName(const string name) {
	this->name = name;
}
virtual const string BaseItem::getName() {
	return this->name;
}
virtual void BaseItem::setPrice(const double price) {
	this->price = price;
}
virtual const double BaseItem::getPrice() {
	return this->name;
}
virtual const string BaseItem::info() {
	return this->info;
}
virtual void BaseItem::show() {

}

}
